<% include ../partials/header %>

<div class="container">
  <h1 class="mb-3">Pitching Search Results</h1>
  <p class="lead">Tables can be sorted by individual columns, just click on the header. You can also search for a specific player, position, or team.</p>

  <table id="pitcher-projections" class="table table-sm table-hover table-striped">
    <thead class="thead-dark">
      <tr>
        <th>Player</th>
        <th>Team</th>
        <th>Games</th>
        <th>GS</th>
        <th>QS</th>
        <th>IP</th>
        <th>K</th>
        <th>HLD</th>
        <th>SV</th>
        <th>ERA</th>
        <th>WHIP</th>
      </tr>
    </thead>
    <tbody>
      <% pitchers.forEach(function(pitcher) { %> 
      <tr>
        <td><%= pitcher.Player %></td>
        <td><%= pitcher.Team %></td>
        <td><%= pitcher.G %></td>
        <td><%= pitcher.GS %></td>
        <td><%= pitcher.QS %></td>
        <td><%= pitcher.IP %></td>
        <td><%= pitcher.K %></td>
        <td><%= pitcher.HLD %></td>
        <td><%= pitcher.SV %></td>
        <td><%= pitcher.ERA %></td>
        <td><%= pitcher.WHIP %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<div class="container">
  <a class="btn btn-primary" href="/search/pitching" role="button">New Search</a>
</div>

<% include ../partials/footer %>